{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "app/**/*.jsx",
    "app/**/*.tsx",
    "components/**/*.jsx",
    "components/**/*.tsx"
  ]
}
